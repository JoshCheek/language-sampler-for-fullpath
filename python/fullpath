#!/usr/bin/env python
import sys
import os

# sys.stdin
# sys.stdout

def normalize(lines):
    return map(lambda x:x.strip(), lines)

def printLines(lines):
    if len(lines) == 1:
        sys.stdout.write(os.path.abspath(lines[0]))
    else:
        for path in lines:
            print(os.path.abspath(path))

args = sys.argv[1:]
if len(args) == 0:
    lines = normalize(sys.stdin.readlines())
    printLines(lines)
else:
    printLines(normalize(args))
